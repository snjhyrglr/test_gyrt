<div id="<%= dom_id geo_barangay %>">
  <p>
    <strong>Geo region:</strong>
    <%= geo_barangay.geo_region_id %>
  </p>

  <p>
    <strong>Geo province:</strong>
    <%= geo_barangay.geo_province_id %>
  </p>

  <p>
    <strong>Geo municipality:</strong>
    <%= geo_barangay.geo_municipality_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= geo_barangay.name %>
  </p>

  <p>
    <strong>Psgc code:</strong>
    <%= geo_barangay.psgc_code %>
  </p>

</div>
